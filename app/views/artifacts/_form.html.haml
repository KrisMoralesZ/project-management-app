= form_for [@project, @artifact] do |f|
  - if @artifact.errors.any?
    #error_explanation
      %h2= "#{pluralize(@artifact.errors.count, "error")} prohibited this artifact from being saved:"
      %ul
        - @artifact.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :key
    = f.text_field :key
  .field
    = f.label :project
    = f.text_field :project
  .actions
    = f.submit 'Save'
